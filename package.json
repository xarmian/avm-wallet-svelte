{
	"name": "avm-wallet-svelte",
	"version": "0.3.5",
	"description": "AVM Wallet Web3 Svelte Component for Algoand-based Networks",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.6.0",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/package": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.9",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"lute-connect": "^1.3.0",
		"vite": "^5.3.5"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@agoralabs-sh/algorand-provider": "^1.1.0",
		"@blockshake/defly-connect": "^1.1.6",
		"@noble/ed25519": "^2.1.0",
		"@noble/hashes": "^1.4.0",
		"@perawallet/connect": "^1.3.4",
		"@sveltejs/kit": "^2.5.18",
		"@types/js-cookie": "^3.0.6",
		"@walletconnect/modal": "^2.6.2",
		"@walletconnect/sign-client": "^2.15.0",
		"@walletconnect/types": "^2.15.0",
		"@walletconnect/utils": "^2.15.0",
		"algosdk": "^2.8.0",
		"autoprefixer": "^10.4.19",
		"esm-env": "^1.0.0",
		"js-cookie": "^3.0.5",
		"postcss": "^8.4.40",
		"postcss-cli": "^11.0.0",
		"svelte-preprocess": "^6.0.2",
		"tailwindcss": "^3.4.7"
	}
}
