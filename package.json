{
	"name": "avm-wallet-svelte",
	"version": "1.1.1",
	"description": "AVM Wallet Web3 Svelte Component for Algorand-based Networks",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./styles/theme.css": "./dist/styles/theme.css"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@fortawesome/fontawesome-free": "^6.7.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.15.0",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@typescript-eslint/eslint-plugin": "^8.18.0",
		"@typescript-eslint/parser": "^8.18.0",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^10.0.0",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^15.14.0",
		"prettier": "^3.4.0",
		"prettier-plugin-svelte": "^3.3.0",
		"publint": "^0.3.0",
		"svelte": "^5",
		"svelte-check": "^4.1.0",
		"tslib": "^2.8.0",
		"typescript": "^5.7.0",
		"typescript-eslint": "^8.18.0",
		"vite": "^5.4.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@agoralabs-sh/avm-web-provider": "^1.7.0",
		"@blockshake/defly-connect": "^1.2.1",
		"@noble/ed25519": "^2.2.0",
		"@noble/hashes": "^1.7.0",
		"@perawallet/connect": "^1.4.2",
		"@types/js-cookie": "^3.0.6",
		"@types/qrcode": "^1.5.5",
		"@walletconnect/universal-provider": "^2.18.0",
		"@zerodevx/svelte-toast": "^0.9.6",
		"algosdk": "^3.5.2",
		"autoprefixer": "^10.4.20",
		"esm-env": "^1.2.0",
		"js-cookie": "^3.0.5",
		"lute-connect": "^1.6.3",
		"postcss": "^8.5.0",
		"postcss-cli": "^11.0.0",
		"qrcode": "^1.5.4",
		"tailwindcss": "^3.4.17",
		"ulujs": "^3.0.2"
	}
}
